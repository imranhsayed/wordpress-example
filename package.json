{
  "name": "wordpress-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "pnpm": ">=10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpm -r run lint",
    "format": "pnpm -r run format",
    "dev": "pnpm -r run dev",
    "dev:hot": "pnpm -r run dev:hot",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "test:watch": "pnpm -r run test:watch",
    "test:coverage": "pnpm -r run test:coverage",
    "test:e2e": "pnpm -r run test:e2e",
    "test:e2e:headed": "pnpm -r run test:e2e:headed",
    "test:e2e:ui": "pnpm -r run test:e2e:ui",
    "test:e2e:debug": "pnpm -r run test:e2e:debug"
  },
  "workspaces": [
    "themes/aquila*"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18",
        "react-dom": "18"
      }
    },
    "allowedDeprecatedVersions": {
      "eslint": "8",
      "@humanwhocodes/config-array": "*",
      "@humanwhocodes/object-schema": "*",
      "glob": "*",
      "inflight": "*",
      "rimraf": "*"
    }
  },
  "devDependencies": {
    "depcheck": "^1.4.7"
  }
}
