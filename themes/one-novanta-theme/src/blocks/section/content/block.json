{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "one-novanta/section-content",
	"version": "0.1.0",
	"title": "Section: Content",
	"category": "one-novanta",
	"icon": "welcome-widgets-menus",
	"description": "Gather blocks in a layout container.",
	"parent": [ "one-novanta/section" ],
	"supports": {
		"align": [ "wide" ]
	},
	"example": {
		"innerBlocks": [
			{
				"name": "core/paragraph",
				"attributes": {
					"content": "Content"
				}
			}
		]
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "wide"
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", "contentOnly", false ]
		}
	},
	"keywords": [ "section", "container", "layout", "novanta" ],
	"textdomain": "one-novanta-theme",
	"editorScript": "file:./index.js"
}
