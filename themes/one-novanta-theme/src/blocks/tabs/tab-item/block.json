{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "one-novanta/tab-item",
    "title": "Tab",
    "description": "Add a single tab",
    "parent": [
        "one-novanta/tabs"
    ],
    "category": "one-novanta",
    "textdomain": "one-novanta-theme",
    "supports": {
        "alignWide": false,
        "anchor": false,
        "className": false,
        "html": false,
        "customClassName": true
    },
	"example": {
		"attributes": {
			"title": "Title"
		}
	},
    "usesContext": [
        "one-novanta/tabs"
    ],
    "attributes": {
        "title": {
            "type": "string",
            "default": ""
        }
    },
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css"
}
