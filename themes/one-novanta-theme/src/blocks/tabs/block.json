{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "one-novanta/tabs",
	"title": "Tabs",
	"description": "Add a set of tabs with content",
	"category": "one-novanta",
	"textdomain": "one-novanta-theme",
	"supports": {
		"alignWide": false,
		"anchor": false,
		"className": false,
		"html": false,
		"customClassName": true,
		"inserter": false
	},
	"allowedBlocks": ["one-novanta/tab"],
	"providesContext": {
		"one-novanta/tabs": "activeTabClientId"
	},
	"attributes": {
		"defaultTabIndex": {
			"type": "number",
			"default": 1
		}
	},
	"editorScript": "file:./index.js",
	"render": "file:./render.php",
	"editorStyle": "file:./index.css"
}
