{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "one-novanta/fifty-fifty-wrapper",
	"version": "0.1.0",
	"title": "50-50 Wrapper",
	"category": "one-novanta",
	"icon": "format-image",
	"description": "50-50 with a content section.",
	"example": {},
	"textdomain": "one-novanta-theme",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"supports": {
		"align": ["full"],
		"inserter": false
	},
	"attributes": {
		"verticalAlign": {
			"type": "string",
			"default": "top",
			"enum": ["top", "center", "bottom"]
		},
		"reverseMobileOrder": {
			"type": "boolean",
			"default": false
		},
		"imageAlignment": {
			"type": "string",
			"default": "right"
		},
		"heading": {
			"type": "string"
		},
		"imageID": {
			"type": "number"
		},
		"imageURL": {
			"type": "string",
			"default": ""
		},
		"template": {
			"type": "string"
		}
	}
}
