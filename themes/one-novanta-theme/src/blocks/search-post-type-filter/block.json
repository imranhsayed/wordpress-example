{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "one-novanta/search-post-type-filter",
	"version": "0.1.0",
	"title": "Post Types Filter",
	"category": "one-novanta",
	"example": {
		"attributes": {
			"selectedPostTypes": [
				{
					"slug": "post",
					"title": "Blogs"
				},
				{
					"slug": "product",
					"title": "Products"
				}
			]
		}
	},
	"icon": "filter",
	"description": "A custom Post type filter block for search page. This will show selected post types with search query link with post type as query parameters.",
	"attributes": {
		"selectedPostTypes": {
			"type": "array",
			"default": []
		}
	},
	"textdomain": "one-novanta-theme",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
