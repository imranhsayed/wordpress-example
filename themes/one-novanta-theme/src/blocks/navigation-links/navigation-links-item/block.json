{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "one-novanta/navigation-links-item",
	"version": "0.1.0",
	"title": "Navigation Links List Item",
	"category": "media",
	"icon": "admin-links",
	"description": "A custom Navigation Links block for list items with links.",
	"parent": [
		"one-novanta/navigation-links"
	],
	"example": {
		"attributes": {
			"title": "New Item",
			"url": "#",
			"opensInNewTab": ""
		}
	},
	"supports": {
		"html": false,
		"reusable": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "New item"
		},
		"url": {
			"type": "string",
			"default": ""
		},
		"opensInNewTab": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "one-novanta-theme",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
