{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "one-novanta/navigation-links",
	"version": "0.1.0",
	"title": "Navigation Links List",
	"category": "media",
	"icon": "editor-ul",
	"description": "A custom Navigation Links block for list items with links.",
	"example": {
		"attributes": {
			"align": "wide",
			"className": "one-novanta-navigation-links",
			"heading": "Frequent Links"
		},
		"innerBlocks": [
			{
				"name": "one-novanta/navigation-links-item",
				"attributes": {
					"title": "List Item 1",
					"url": "#",
					"opensInNewTab" : ""
				}
			},
			{
				"name": "one-novanta/navigation-links-item",
				"attributes": {
					"title": "List Item 2",
					"url": "#",
					"opensInNewTab" : ""
				}
			}
		]
	},
	"supports": {},
	"attributes": {
		"heading": {
			"type": "string",
			"default": ""
		},
		"className": {
			"type": "string",
			"default": "one-novanta-navigation-links"
		}
	},
	"textdomain": "one-novanta-theme",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
