	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": false,
		"dimensions": {
			"aspectRatios": [
				{
					"name": "16:9",
					"slug": "16-9",
					"ratio": "1.7777777778"
				},
				{
					"name": "4:3",
					"slug": "4-3",
					"ratio": "1.3333333333"
				},
				{
					"name": "1:1",
					"slug": "1-1",
					"ratio": "1"
				}
			],
			"defaultAspectRatios": false
		},
		"blocks": {
			"core/button": {
				"typography": {
					"defaultFontSizes": false,
					"customFontSize": false,
					"fontStyle": false,
					"fontWeight": false,
					"letterSpacing": false,
					"lineHeight": false,
					"textDecoration": false,
					"textTransform": false,
					"fontFamilies": [],
					"fontSizes": []
				}
			},
			"one-novanta/section": {
				"spacing": {
					"padding": true,
					"defaultSpacingSizes": false,
					"spacingSizes": [
						{
							"name": "Small",
							"size": "clamp(60px, 7vw, 80px)",
							"slug": "80"
						},
						{
							"name": "Medium",
							"size": "clamp(70px, 8vw, 100px)",
							"slug": "100"
						},
						{
							"name": "Large",
							"size": "clamp(80px, 8vw, 120px)",
							"slug": "120"
						}
					]
				},
				"color": {
					"background": true,
					"palette": [
						{
							"slug": "background",
							"color": "#fff",
							"name": "Background"
						},
						{
							"slug": "secondary",
							"color": "#f5f5f5",
							"name": "Secondary"
						}
					]
				}
			}
		},
		"color": {
			"defaultPalette": false,
			"text": false,
			"background": true,
			"link": false,
			"button": false,
			"heading": false,
			"caption": false,
			"customDuotone": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"custom": false,
			"customGradient": false,
			"gradients": [],
			"palette": [
				{
					"slug": "foreground",
					"color": "#000",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#fff",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#d9272d",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#f5f5f5",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#9a9a9a",
					"name": "Tertiary"
				},
				{
					"slug": "quaternary",
					"color": "#ececec",
					"name": "Quaternary"
				},
				{
					"slug": "quinary",
					"color": "#005fcc",
					"name": "Quinary"
				},
				{
					"slug": "primary-blue",
					"color": "#004686",
					"name": "Primary Blue"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Source Serif Pro\", serif",
					"name": "Source Serif Pro",
					"slug": "source-serif-pro"
				},
				{
					"fontFamily": "Lato, sans-serif",
					"name": "Body Font",
					"slug": "body",
					"fontFace": [
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "300",
							"src": ["file:./src/fonts/lato/light.woff2"]
						},
						{
							"fontFamily": "Lato",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontWeight": "300",
							"src": ["file:./src/fonts/lato/light-italic.woff2"]
						},
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "400",
							"src": ["file:./src/fonts/lato/regular.woff2"]
						},
						{
							"fontFamily": "Lato",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontWeight": "400",
							"src": [
								"file:./src/fonts/lato/regular-italic.woff2"
							]
						},
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "700",
							"src": ["file:./src/fonts/lato/bold.woff2"]
						},
						{
							"fontFamily": "Lato",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontWeight": "700",
							"src": ["file:./src/fonts/lato/bold-italic.woff2"]
						}
					]
				},
				{
					"fontFamily": "Monsterrat, sans-serif",
					"name": "Heading Font",
					"slug": "heading",
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"fontWeight": "100 900",
							"src": ["file:./src/fonts/montserrat/regular.woff2"]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"fontWeight": "100 900",
							"src": ["file:./src/fonts/montserrat/italic.woff2"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"size": "10px",
					"slug": "tiny"
				},
				{
					"size": "12px",
					"slug": "small"
				},
				{
					"size": "16px",
					"slug": "normal"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					},
					"size": "20px",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.375rem",
						"max": "1.5rem"
					},
					"size": "24px",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.8rem",
						"max": "2.25rem"
					},
					"size": "36px",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "1.2rem",
						"max": "3rem"
					},
					"size": "48px",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "3.75rem"
					},
					"size": "60px",
					"slug": "xxx-large"
				},
				{
					"fluid": {
						"min": "2.2rem",
						"max": "5.25rem"
					},
					"size": "84px",
					"slug": "display"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "navigation"
				}
			]
		},
		"layout": {
			"contentSize": "clamp(260px, 88vw, 840px)",
			"wideSize": "clamp(260px, 88vw, 1680px)"
		},
		"spacing": {
			"units": ["px", "em", "rem", "vw", "vh", "%"],
			"defaultSpacingSizes": false,
			"blockGap": true,
			"customSpacingSize": false,
			"margin": true,
			"padding": true,
			"spacingSizes": [
				{
					"name": "1",
					"size": "10px",
					"slug": "10"
				},
				{
					"name": "1.5",
					"size": "15px",
					"slug": "15"
				},
				{
					"name": "2",
					"size": "20px",
					"slug": "20"
				},
				{
					"name": "3",
					"size": "clamp(24px, 2.5vw, 30px)",
					"slug": "30"
				},
				{
					"name": "4",
					"size": "clamp(30px, 3.5vw, 40px)",
					"slug": "40"
				},
				{
					"name": "5",
					"size": "clamp(40px, 4.4vw, 50px)",
					"slug": "50"
				},
				{
					"name": "6",
					"size": "clamp(48px, 5.4vw, 60px)",
					"slug": "60"
				},
				{
					"name": "7",
					"size": "clamp(54px, 6.2vw, 70px)",
					"slug": "70"
				},
				{
					"name": "8",
					"size": "clamp(60px, 7vw, 80px)",
					"slug": "80"
				},
				{
					"name": "9",
					"size": "clamp(70px, 7vw, 90px)",
					"slug": "90"
				},
				{
					"name": "10",
					"size": "clamp(70px, 8vw, 100px)",
					"slug": "100"
				},
				{
					"name": "12",
					"size": "clamp(80px, 8vw, 120px)",
					"slug": "120"
				},
				{
					"name": "typography",
					"size": "20px",
					"slug": "typography"
				}
			]
		},
		"custom": {
			"borderRadiusSmall": "5px",
			"borderRadiusNormal": "10px",
			"borderRadiusMedium": "20px",
			"borderRadiusLarge": "50px",
			"borderRadiusExtraLarge": "100px",
			"buttonLineHeight": "1",
			"colorSnowWhite": "#fbfbfb",
			"colorAshGray": "#b8b4b4",
			"colorDarkGray": "#555050",
			"colorLightGreen": "#93cc60",
			"selectDropDownBorderColor": "#caced1",
			"selectDropDownScrollBarTrackColor": "#f1f1f1",
			"selectDropDownScrollBarThumbColor": "#cccccc",
			"selectDropDownSelectedBackground": "#f2f2f2",
			"colorMidGray": "#808080",
			"colorSilverGray": "#a7a7a7",
			"colorCloudGray": "#c3c3c3",
			"colorGraphiteGray": "#8d8d8d",
			"colorError": "#d32f2f"
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"shadow": {
			"defaultPresets": false
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "20px",
			"margin": { "top": "0", "bottom": "0" }
		},
		"blocks": {
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--40)"
					},
					"blockGap": "var(--wp--preset--spacing--30)"
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "100",
					"textTransform": "uppercase"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "0"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--medium)",
						"bottom": "var(--wp--custom--spacing--medium)"
					}
				},
				"typography": {
					"fontWeight": "100",
					"lineHeight": "1.2"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--foreground)"
				},
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "400",
					"textTransform": "uppercase"
				}
			},
			"core/list": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--20)"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "600"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "600"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "600"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "600"
				}
			},
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--background)"
					}
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontWeight": "400",
			"lineHeight": "1.1875"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
